<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.phonegap.example"
    versionCode = "10"
    version     = "1.0.0" >

<!-- versionCode is optional and Android only -->

  <name>PhoneGap Example</name>

  <description>
      An example for phonegap build docs.
  </description> 

  <author href="https://build.phonegap.com" email="support@phonegap.com">
      Hardeep Shoker  
  </author>
  <!--
    This allows your app to load assets from all *.phongap.com domains
  -->
  <access origin="http://phonegap.com" subdomains="true" />
  
  <!-- for Android -->
<feature name="Flashlight">
  <param name="android-package" value="nl.xservices.plugins.Flashlight" />
</feature>
  <gap:plugin name="nl.x-services.plugins.flashlight" />
  
  <!-- streaming video plugin -->
  <plugin name="com.hutchind.cordova.plugins.streamingmedia" spec="0.1.3" source="pgb" />

  
</widget>